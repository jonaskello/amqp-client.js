{
  "name": "@jonaskello-forks/amqp-client",
  "version": "1.3.5",
  "description": "AMQP 0-9-1 client, both for browsers (WebSocket) and node (TCP Socket)",
  "main": "lib/amqp-client.cjs",
  "types": "lib/amqp-client.d.ts",
  "browser": "lib/amqp-websocket-client.mjs",
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "docs": "typedoc --entryPoints src/index.ts --tsconfig tsconfig.typedoc.json",
    "lint": "eslint src",
    "test": "c8 ava",
    "clean": "rm -rf lib",
    "build": "tsc",
    "prepack": "npm run clean && npm run build",
    "preversion": "npm test",
    "version": "npm run build"
  },
  "files": [
    "src/",
    "lib/"
  ],
  "repository": "github:cloudamqp/amqp-client.js",
  "keywords": [
    "amqp",
    "rabbitmq"
  ],
  "author": "CloudAMQP <contact@cloudamqp.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/cloudamqp/amqp-client.js/issues"
  },
  "homepage": "https://github.com/cloudamqp/amqp-client.js#readme",
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/node": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "ava": "^3.15.0",
    "c8": "^7.10.0",
    "eslint": "^8.5.0",
    "rollup": "^2.39.1",
    "ts-node": "^10.4.0",
    "typedoc": "^0.22.10",
    "typescript": "^4.5.4"
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nonSemVerExperiments": {
      "configurableModuleFormat": true
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  }
}
